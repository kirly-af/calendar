{
  "extends": "standard",

  "env": {
    "browser": true,
    "mocha": true,
    "node": false
  },

  "globals": {
    "document": true,
    "window": true,
    "angular": true
  },

  "rules": {
    "brace-style": [1, "allman"],
    "curly": [1, "multi"],
    "no-trailing-spaces": 1,
    "no-unused-vars": 1,
    "semi": [2, "always"],
    "space-before-function-paren": [1, "never"]
  }
}
