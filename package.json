{
  "name": "es6-jspm-gulp-boilerplate",
  "version": "0.4.2",
  "description": "Boilerplate for ES6+ apps using JSPM + Babel",
  "main": "dist/index.html",
  "homepage": "https://github.com/alexweber/es6-jspm-gulp-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexweber/es6-jspm-gulp-boilerplate.git"
  },
  "bugs": "https://github.com/alexweber/es6-jspm-gulp-boilerplate/issues",
  "author": {
    "name": "Alex Weber",
    "email": "alexweber15@gmail.com",
    "url": "http://alexweber.com.br"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp --gulpfile Gulpfile.coffee",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc app/**/*.scss",
    "postinstall": "jspm install"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "boilerplate",
    "babel",
    "systemjs"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "jspm_packages"
  },
  "devDependencies": {
    "browser-sync": "^2.12.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "coffee-script": "^1.10.0",
    "cson": "^3.0.2",
    "del": "^2.2.0",
    "dotenv": "^2.0.0",
    "esdoc": "^0.4.4",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-data": "^1.2.1",
    "gulp-eslint": "^1.1.1",
    "gulp-filter": "^4.0.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jspm": "^0.5.6",
    "gulp-load-plugins": "^1.2.2",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-pug": "^1.3.0",
    "gulp-pug-lint": "^0.1.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-sass-lint": "^1.1.1",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "imagemin-pngquant": "^4.2.0",
    "jspm": "^0.16.33",
    "jspm-bower-endpoint": "^0.3.2",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-sinon-chai": "^1.1.0",
    "merge-stream": "^1.0.0",
    "mocha": "^2.4.5",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-alpha6",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "sass-jspm-importer": "github:idcware/node-sass-jspm-importer",
    "sassdoc": "^2.1.20",
    "yargs": "^4.6.0"
  },
  "jspm": {
    "directories": {
      "doc": "docs",
      "test": "test"
    },
    "configFile": "jspm.conf.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.5",
      "angular-ui-calendar": "github:angular-ui/ui-calendar@^1.0.1",
      "font-awesome": "npm:font-awesome@^4.6.1",
      "fullcalendar": "npm:fullcalendar@^2.7.0-beta",
      "jquery": "npm:jquery@^2.2.3",
      "moment": "npm:moment@^2.13.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "dependencies": {
    "@angular/router": "^0.2.0",
    "angular": "^1.5.5"
  }
}
